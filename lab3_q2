#include <stdio.h>

int main(){

int mark;

printf(" Enter mark: ");
scanf("%d", &mark);

if ( mark >= 85 && mark <= 100)
printf(" Your grade is: HD\n");

else if ( mark >= 75 && mark <=84)
printf(" Your grade is: D\n");

else if ( mark >= 65 && mark <= 74)
printf(" Your grade is: C\n");

else if ( mark >= 50 && mark <= 64)
printf(" Your grade is; P\n");

else
printf("Your grade is: F");

return 0;
}
